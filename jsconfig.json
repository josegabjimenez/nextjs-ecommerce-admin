{
  "compilerOptions": {
    "target": "es2017",
    "allowSyntheticDefaultImports": false,
    "baseUrl": "src",
    "paths": {
      "@components/*": ["components/*"],
      "@containers/*": ["containers/*"],
      "@pages/*": ["pages/*"],
      "@layout/*": ["layout/*"],
      "@styles/*": ["styles/*"],
      "@config/*": ["config/*"],
      "@assets/*": ["assets/*"],
      "@hooks/*": ["hooks/*"],
      "@services/*": ["services/*"]
    }
  },
  "exclude": ["node_modules", "dist"]
}
